{
    "name": "keypix-ui",
    "private": false,
    "version": "1.0.1",
    "description": "A premium React component library built with TypeScript, Tailwind CSS, and Atomic Design principles. The most comprehensive and beautiful UI library available.",
    "author": {
        "name": "Keypix Technologies",
        "email": "keypixteam@gmail.com",
        "url": "https://ui.keypix.uz"
    },
    "license": "MIT",
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/keypix-tech"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "sideEffects": false,
    "unpkg": "./dist/index.js",
    "jsdelivr": "./dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsup",
        "build:clean": "rm -rf dist && npm run build",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint . --ext .ts,.tsx --fix",
        "preview": "vite preview",
        "type-check": "tsc --noEmit",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint && npm run type-check",
        "docs:dev": "vite --config docs/vite.config.ts",
        "docs:build": "vite build --config docs/vite.config.ts",
        "docs:preview": "vite preview --config docs/vite.config.ts"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "tailwindcss": ">=4.0.0"
    },
    "peerDependenciesMeta": {
        "tailwindcss": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "dependencies": {
        "clsx": "^2.1.1",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@tailwindcss/vite": "^4.1.11",
        "@types/node": "^24.1.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.6.0",
        "autoprefixer": "^10.4.21",
        "class-variance-authority": "^0.7.1",
        "eslint": "^9.30.1",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.11",
        "tsup": "^8.5.0",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^7.0.4"
    },
    "keywords": [
        "react",
        "typescript",
        "ui",
        "components",
        "design-system",
        "tailwindcss",
        "atomic-design",
        "dark-mode",
        "accessibility",
        "component-library",
        "ui-kit",
        "react-components",
        "typescript-components",
        "modern-ui",
        "premium-ui",
        "enterprise-ui",
        "responsive-design",
        "mobile-first",
        "css-in-js",
        "utility-first"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/keypix-tech/ui.git"
    },
    "bugs": {
        "url": "https://github.com/keypix-tech/ui/issues"
    },
    "homepage": "https://ui.keypix.uz",
    "docs": "https://ui.keypix.uz/docs",
    "changelog": "https://github.com/keypix-tech/ui/blob/main/CHANGELOG.md",
    "contributors": [{
        "name": "KeyPix Team",
        "email": "keypixteam@gmail.com",
        "url": "https://keypix.uz"
    }],
    "maintainers": [{
        "name": "KeyPix Team",
        "email": "keypixteam@gmail.com",
        "url": "https://ui.keypix.uz"
    }],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}